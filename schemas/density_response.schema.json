{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DensityResponse",
  "type": "object",
  "required": [
    "ok",
    "engine",
    "segments"
  ],
  "properties": {
    "ok": {
      "type": "boolean"
    },
    "engine": {
      "type": "string",
      "const": "density"
    },
    "segments": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "segment",
          "steps",
          "peak",
          "time_in_zones"
        ],
        "properties": {
          "segment": {
            "type": "object"
          },
          "steps": {
            "type": "array",
            "items": {
              "type": "object"
            }
          },
          "peak": {
            "type": "object"
          },
          "time_in_zones": {
            "type": "object"
          }
        }
      }
    }
  }
}